<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <rocketweb_checkout_enhancement>
            <payment>
                <active_default_payment>1</active_default_payment>
                <default_method>authnetcim</default_method>
            </payment>
            <google_map>
                <api_key />
            </google_map>
            <google_map_address>
                <active_google_address>1</active_google_address>
                <libraries><![CDATA[places]]></libraries>
                <country><![CDATA[US,CA]]></country>
                <language><![CDATA[en_US]]></language>
                <active_google_address_account>1</active_google_address_account>
                <active_google_address_billing>1</active_google_address_billing>
            </google_map_address>
        </rocketweb_checkout_enhancement>
    </default>
</config>
